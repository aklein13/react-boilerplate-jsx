    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-responsive": "^1.3.4",
    "rimraf": "^2.5.4",
    "sass-lint": "^1.9.1",
    "sass-loader": "^6.0.0",
    "source-map-loader": "^0.1.6",
    "style-loader": "^0.13.1",
    "uglify-js": "^2.7.5",
    "url-loader": "^0.5.7",
