{
  "name": "boilerplate",
  "version": "2.0.0-beta.3",
  "description": "Boilerplate",
  "homepage": "https://boiler-plate.com/",
  "author": {
    "name": "Arkadiusz Klein",
    "email": "arek.klein@gmail.com"
  },
  "keywords": [
    "react",
    "redux",
    "jest",
    "scss",
    "sass",
    "boilerplate",
    "webpack-dev-server",
    "yarn",
    "hmr"
  ],
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "cross-env NODE_ENV=production webpack --display-error-details --config webpack-production.config.js && uglifyjs ./assets/vendor.js > dist/vendor.js && rm ./dist/*.map",
    "buildvendor": "webpack --config webpack-vendor.config.js",
    "clean": "rimraf dist",
    "lint": "tslint --force \"src/**/*.ts*\" && sass-lint -v -q src/**/*.scss",
    "ghpages": "minify assets/index.html > dist/index.html && gh-pages -d dist"
  },
  "engines": {
    "npm": ">=3.10.0",
    "node": ">=6.9.0",
    "yarn": ">=0.16.0"
  },
  "dependencies": {
    "@types/redux-promise": "^0.5.24",
    "axios": "^0.16.2",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "lodash": "^4.17.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@types/jest": "^18.1.1",
    "@types/node": "6.0.52",
    "@types/react": "^15.0.7",
    "@types/react-addons-test-utils": "^0.14.17",
    "@types/react-dom": "^0.14.23",
    "@types/react-hot-loader": "^3.0.1",
    "@types/react-redux": "^4.4.36",
    "@types/react-router": "^3.0.3",
    "@types/react-router-redux": "^4.0.40",
    "@types/whatwg-fetch": "0.0.33",
    "babel-preset-stage-1": "^6.1.18",
    "codecov": "^1.0.1",
    "copy-webpack-plugin": "^4.0.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "exports-loader": "^0.6.3",
